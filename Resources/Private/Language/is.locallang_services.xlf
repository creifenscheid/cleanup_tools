        <body>
           <!-- LABEL -->
            <trans-unit id="label.CleanFlexForms">
                <source>FlexForms</source>
                <target>FlexForms</target>
            </trans-unit>
            <trans-unit id="label.DeletedRecords">
                <source>Deleted records</source>
                <target>Gögnum eytt</target>
            </trans-unit>
            <trans-unit id="label.LostFiles">
                <source>Lost files</source>
                <target>Týndar skrár</target>
            </trans-unit>
            <trans-unit id="label.OrphanRecords">
                <source>Orphan records</source>
                <target>Munaðarlaus plötur</target>
            </trans-unit>

            <!-- DESCRIPTIONS -->
            <trans-unit id="description.CleanFlexForms">
                <source>Checks if TCA records with a FlexForm includes values that don't match the connected FlexForm value</source>
                <target>Athugar hvort TCA skrár með FlexForm innihalda gildi sem passa ekki við tengda FlexForm gildi</target>
            </trans-unit>
            <trans-unit id="description.DeletedRecords">
                <source>Force-deletes all records in the database which have a deleted=1 flag</source>
                <target>Eyðir öllum skrám í gagnagrunninum sem hefur eytt = 1 fána</target>
            </trans-unit>
            <trans-unit id="description.LostFiles">
                <source>Finds files within uploads/ which are not needed anymore</source>
                <target>Finnur skrár sem hafa verið hlaðið inn / sem ekki er þörf lengur</target>
            </trans-unit>
            <trans-unit id="description.OrphanRecords">
                <source>Finds (and fixes) all records that have an invalid / deleted page ID</source>
                <target>Finnur (og lagfærir) allar skrár sem hafa ógilt / eytt auðkenni síðu</target>
            </trans-unit>
            
           <!-- PARAMETER -->
            <trans-unit id="label.updateRefindex">
                <source>Update reference index</source>
                <target>Uppfæra viðmiðunarvísitölu</target>
            </trans-unit>
            <trans-unit id="label.recordUid">
                <source>Record uid</source>
                <target>Taktu upp uid</target>
            </trans-unit>
            <trans-unit id="label.pid">
                <source>Page uid</source>
                <target>Síðu uid</target>
            </trans-unit>
            <trans-unit id="label.depth">
                <source>Depth</source>
                <target>Dýpt</target>
            </trans-unit>
            <trans-unit id="label.dryRun">
                <source>Dry run</source>
                <target>Þurrt hlaup</target>
            </trans-unit>
            <trans-unit id="label.exclude">
                <source>Paths that should be excluded</source>
                <target>Stígar sem ætti að útiloka</target>
            </trans-unit>
            <trans-unit id="label.customPath">
                <source>Paths to process (default: uploads)</source>
                <target>Slóð til að vinna úr (sjálfgefið: uploads)</target>
            </trans-unit>
        </body>
