        <body>
            <!-- DESCRIPTIONS -->
            <trans-unit id="description.CleanFlexFormsService">
                <source>Checks if TCA records with a FlexForm includes values that don't match the connected FlexForm value</source>
                <target>Überprüft, ob TCA-Einträge mit einer FlexForm Werte enthalten, die nicht mit dem verbundenen FlexForm-Wert übereinstimmen</target>
            </trans-unit>
            <trans-unit id="description.DeletedRecordsService">
                <source>Force-deletes all records in the database which have a deleted=1 flag</source>
                <target>Löscht alle Datensätze in der Datenbank, die ein delete=1-Flag haben</target>
            </trans-unit>
            <trans-unit id="description.LostFilesService">
                <source>Finds files within uploads/ which are not needed anymore</source>
                <target>Findet Dateien in uploads/, die nicht mehr benötigt werden</target>
            </trans-unit>
            <trans-unit id="description.OrphanRecordsService">
                <source>Finds (and fixes) all records that have an invalid / deleted page ID</source>
                <target>Findet (und behebt) alle Datensätze, die eine ungültige / gelöschte Seiten-ID haben</target>
            </trans-unit>
            
           <!-- PARAMETER -->
            <trans-unit id="label.updateRefindex">
                <source>Update reference index</source>
                <target>Referenzindex aktualisieren</target>
            </trans-unit>
            <trans-unit id="label.recordUid">
                <source>Record uid</source>
                <target>Element-ID</target>
            </trans-unit>
            <trans-unit id="label.pid">
                <source>Page uid</source>
                <target>Seiten-ID</target>
            </trans-unit>
            <trans-unit id="label.depth">
                <source>Depth</source>
                <target>Tiefe</target>
            </trans-unit>
            <trans-unit id="label.dryRun">
                <source>Dry run</source>
                <target>Trockenübung</target>
            </trans-unit>
            <trans-unit id="label.exclude">
                <source>Paths that should be excluded (comma-separated)</source>
                <target>Auszuschließende Pfade (komma-separiert)</target>
            </trans-unit>
            <trans-unit id="label.customPath">
                <source>Paths to process (default: uploads) (comma-separated)</source>
                <target>Zu bereinigende Pfade (komma-separiert)</target>
            </trans-unit>
            
            <!-- CONFIGURATION -->
            <trans-unit id="module.history.cleanupForm.label.logLiftime.1-day">
