            <trans-unit id="module.history.cleanupForm.label.logLiftime.6-months">
                <source>6 months</source>
                <target>6 Monate</target>
            </trans-unit>
            <trans-unit id="module.history.cleanupForm.label.logLiftime.1-year">
                <source>1 year</source>
                <target>1 Jahr</target>
            </trans-unit>
